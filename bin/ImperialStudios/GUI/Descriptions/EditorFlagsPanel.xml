<Panel name           = "EditorFlagsPanel"
       layout         = "XAXIS"
       anchorPoint    = "CORNERLU"
       drawBounds     = "false"
       drawBackground = "false"
       clipSize       = "0"
       visible        = "true">
  
  <Position x = "2" y = "2" />
  <BordersColor x = "0.3" y = "0.3" z = "0.3" />
  <BGColor x = "20" y = "20" z = "20" w = "1" />
  
  <ComboBox callbackString = "BrushTypesCB" clipSize = "0">
    <ScrollingColor x = "88" y = "76" z = "30" />
    <BordersColor x = "102" y = "90" z = "34" />
    <BGColor x = "20" y = "20" z = "20" w = "1" />
    <Button name = "cbbutton0" drawBounds = "true" clipSize = "0" >
      <BordersColor x = "88" y = "76" z = "30" />
      <Color x = "68" y = "56" z = "10" w = "1" />
    </Button>
    <Item string = "Arrow" />
    <Item string = "Paint" />
    <Item string = "Fill" />
    <Item string = "Marker" />
    <Item string = "Pastel" />
    <Item string = "Raise" />
    <Item string = "Lower" />
    <Item string = "Erase" />
    <Item string = "Restore" />
    <Item string = "Burn" />
    <Item string = "Heal" />
    <Item string = "Tile erase" />
    <Item string = "Tile logic" />
    <Item string = "Tile flag" />
    <Item string = "Advanced" />
  </ComboBox>

  <ComboBox callbackString = "TileOrientationCB" clipSize = "0">
