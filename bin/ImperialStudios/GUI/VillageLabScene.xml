<VillageLabScene>
	
  <Controller>
    
    <Command type = "change" source = "CancelButton" target = "EditorScene"/>
    <Command type = "change" source = "AcceptButton" target = "EditorScene"/>
    
  </Controller>
  
  <Panel visible = "false" name = "Village Lab">
    
    <Font path = "ArialSmall.tga" />
    <Texture description = "GUIElements.xml" />
    
    <Panel description = "OKCancelPanel.xml" />
    <Panel description = "GeosetPanel.xml" />
    <Panel description = "ModelPropsPanel.xml" />
    
    <Panel name = "ModelPropertiesPanel" layout = "YAXIS" anchorPoint = "CORNERRU" clipSize = "0">
      
      <Position x = "-10" y = "10" />
      
      <Panel description = "PropsWindowPanel.xml" />
      
      <Panel name = "VillagePropsPanel" layout = "YAXIS">
        
        <Interval x = "0" y = "0" />
        
        <Panel name = "TopPopulationPanel" layout = "XAXIS" drawBounds = "false" drawBackground = "false">
          <Label name = "Population">    
            <Text string = "Population" />
          </Label>

          <TextBox callbackString = "PopulationTextbox">
            <Dimensions x = "60" />
            <Text string = "400" />
          </TextBox>
        </Panel>
        
        <Panel name = "BottomPopulationPanel" layout = "XAXIS" drawBounds = "false" drawBackground = "false">
          <Label name = "Max population">    
            <Text string = "Max population" />
          </Label>

          <TextBox callbackString = "MaxPopulationTextbox">
            <Dimensions x = "60" />
            <Text string = "400" />
          </TextBox>
        </Panel>
        
      </Panel>
      
    </Panel>
    
    <TexCoordsDesc xStart = "201" yStart = "2"   xEnd = "233" yEnd = "34"  type = "SLIDER"/>
    <TexCoordsDesc xStart = "1"   yStart = "136" xEnd = "21"  yEnd = "155" type = "CHECK_BOX"/>
    <TexCoordsDesc xStart = "25"  yStart = "132" xEnd = "49"  yEnd = "155" type = "CHECK_BOX_MARK"/>
    <TexCoordsDesc xStart = "1"   yStart = "1"   xEnd = "65"  yEnd = "66"  type = "RADIO_BUTTON"/>
    <TexCoordsDesc xStart = "1"   yStart = "2"   xEnd = "130" yEnd = "66"  type = "CHECK_RB_MARK"/>
    
  </Panel>
  
</VillageLabScene>
